{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@app/assets/*": [
                "src/assets/*"
            ],
            "@app/components/*": [
                "src/components/*"
            ],
            "@app/constants/*": [
                "src/constants/*"
            ],
            "@app/navigations/*": [
                "src/navigations/*"
            ],
            "@app/screens/*": [
